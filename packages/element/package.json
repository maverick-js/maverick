{
  "name": "@maverick-js/element",
  "license": "MIT",
  "version": "0.43.2",
  "type": "module",
  "types": "src/index.ts",
  "module": "src/index.ts",
  "sideEffects": false,
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maverick-js/maverick.git",
    "directory": "packages/element"
  },
  "bugs": {
    "url": "https://github.com/maverick-js/maverick/issues"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "pnpm clean && pnpm types && rollup -c",
    "clean": "rimraf dist-npm",
    "types": "tsc -p tsconfig.build.json",
    "test": "pnpm test:client && pnpm test:server",
    "format": "prettier src --write --loglevel warn",
    "test:client": "vitest --run",
    "test:client:watch": "vitest --watch",
    "test:server": "SERVER=true vitest --run",
    "test:server:watch": "SERVER=true vitest --watch"
  },
  "dependencies": {
    "@maverick-js/dom": "workspace:*",
    "@maverick-js/std": "workspace:*",
    "@maverick-js/ssr": "workspace:*",
    "maverick.js": "workspace:*"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^2.1.2",
    "@maverick-js/signals": "^6.0.0",
    "@maverick-js/ssr": "workspace:*",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "happy-dom": "^15.7.4",
    "rimraf": "^5.0.10",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/server/index.ts"
  },
  "$types": "index.d.ts",
  "$exports": {
    ".": {
      "types": "./index.d.ts",
      "node": "./server/index.js",
      "development": "./dev/index.js",
      "default": "./prod/index.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "node": "./server/server.js",
      "development": "./dev/server.js",
      "default": "./prod/server.js"
    },
    "./types/*": "./types/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
